@model IEnumerable<User>
@*

    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "AllUsers";
}
<h1>Hi</h1>

<section class="text-center">
    <div class="container">
        <h1>Users:</h1>
        @foreach(var item in Model){
        <p>
            <a href="#" class="btn primary m-2">User: @item.Name </a>
            <a href="#" class="btn primary m-2">Password: @item.Password </a>
            <a href="#" class="btn primary m-2">Email: @item.Email </a>
            <a href="#" class="btn primary m-2">Phone number: @item.UserRole.ToString() </a>
            <a href="#" class="btn primary m-2">Enable: @item.IsEnabled </a>
        </p>
        }
    </div>
</section>
